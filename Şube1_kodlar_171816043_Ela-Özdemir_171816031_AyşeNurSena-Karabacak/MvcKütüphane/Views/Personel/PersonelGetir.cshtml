@model MvcKütüphane.Models.Entity.TBLPERSONEL
@{
    ViewBag.Title = "PersonelGetir";
    Layout = "~/Views/Shared/Layout.cshtml";
}

@using (Html.BeginForm("PersonelGuncelle", "Personel", FormMethod.Post))
{
    <div style="margin-bottom:20px"></div>
    <div class="form-group">
        @Html.LabelFor(k => k.ID)@*Label adı neyse o gelsin demek*@
        @Html.TextBoxFor(k => k.ID, new { @class = "form-control" })@*Textboxfor a da o sütündaki değeri yazdırsın*@
        <br />
        @*@Html.LabelFor(k => k.PERSONEL)*@
        <label>Personel Adı</label>

        @Html.TextBoxFor(k => k.PERSONEL, new { @class = "form-control" })
    </div>
    <div>
        <button class="btn btn-warning">Güncelle</button>
    </div>
}

